{
  "name": "rapide/laravel-queue-kafka",
  "description": "Kafka driver for Laravel Queue",
  "license": "MIT",
  "type": "library",
  "version": "1.0",
  "authors": [
    {
      "name": "Peter Mein",
      "email": "petermein@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/database": "^9.19.0",
    "illuminate/support": "^9.19.0",
    "illuminate/queue": "^9.19.0",
    "ext-rdkafka": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "mockery/mockery": "^1.4.4"
  },
  "autoload": {
    "psr-4": {
      "Rapide\\LaravelQueueKafka\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rapide\\LaravelQueueKafka\\LaravelQueueKafkaServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
